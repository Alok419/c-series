#include<stdio.h>

void main(){
    int a[10];
    int b[10];
    int sum1 = 0;
    int sum2 = 0;
    int totalSum ;
    printf(" Enter the values of  array1 of size 10");
    for(int i = 0;i<10;i++){
        scanf("%d",&a[i]);
    }
    
     for(int i = 0;i<10;i++){
        sum1 += a[i];
    }
    
     printf(" Enter the values of  array2 of size 10");
    for(int i = 0;i<10;i++){
        scanf("%d",&b[i]);
    }
    
     for(int i = 0;i<10;i++){
        sum2 += b[i];
    }
    
    totalSum= sum1+sum2;
    printf(" sum is %d",totalSum );
}
